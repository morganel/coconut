<% if flash[:notice] === "success" %>
$('#render_partial_form').html("<%= escape_javascript (render :partial=>'flights/index_seat', :locals=>{:seats=>@seat.flight.seats}) %>");
$("#seat_baggage").val("");
$('#seat_name').val("");
$("#notice").html("<%= flash[:notice] %>");
<% else %>
$("#notice").html("FAILURE");
<% end %>
